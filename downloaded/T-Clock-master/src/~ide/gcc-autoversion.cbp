<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AutoVersion (gcc)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="release">
				<Option output="../common/AutoVersion" prefix_auto="1" extension_auto="1" />
				<Option object_output="../.obj/gcc" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--git . _testver.h" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-fno-ident" />
			<Add option="-Wall" />
			<Add option="-fvisibility=hidden" />
			<Add option="-ffunction-sections" />
			<Add option="-fmerge-all-constants" />
			<Add option="-fno-asynchronous-unwind-tables" />
			<Add option="-fno-exceptions" />
			<Add option="-fwhole-program" />
		</Compiler>
		<Linker>
			<Add option="-static" />
			<Add option="-Wl,--gc-sections" />
		</Linker>
		<Unit filename="../common/autoversion.cpp" />
		<Unit filename="../common/getopt_tools.c">
			<Option compilerVar="CC" />
			<Option compile="0" />
			<Option link="0" />
		</Unit>
		<Unit filename="../common/getopt_tools.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
