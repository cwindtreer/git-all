<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DLL (gcc)" />
		<Option pch_mode="2" />
		<Option compiler="i686-w64-mingw32" />
		<Build>
			<Target title="release-32">
				<Option output="../../Release/misc/T-Clock.dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../.obj/gcc" />
				<Option type="3" />
				<Option compiler="i686-w64-mingw32" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-m32" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-m32" />
				</Linker>
			</Target>
			<Target title="release-64">
				<Option output="../../Release/misc/T-Clock64.dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../.obj64/gcc" />
				<Option type="3" />
				<Option compiler="x86_64-w64-mingw32" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-m64" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-m64" />
				</Linker>
			</Target>
			<Target title="debug-32">
				<Option output="../../Debug/misc/T-Clock.dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../.obj/gcc/dbg" />
				<Option type="3" />
				<Option compiler="i686-w64-mingw32" />
				<Compiler>
					<Add option="-m32" />
					<Add option="-Og" />
					<Add option="-g" />
					<Add option="-fno-omit-frame-pointer" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="-m32" />
				</Linker>
				<ExtraCommands>
					<Add after="objcopy --only-keep-debug ../../Debug/misc/T-Clock.dll ../../Debug/misc/T-Clock.dbg" />
					<Add after="objcopy --strip-debug --strip-unneeded --add-gnu-debuglink=../../Debug/misc/T-Clock.dbg ../../Debug/misc/T-Clock.dll" />
				</ExtraCommands>
			</Target>
			<Target title="debug-64">
				<Option output="../../Debug/misc/T-Clock64.dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="../.obj64/gcc/dbg" />
				<Option type="3" />
				<Option compiler="x86_64-w64-mingw32" />
				<Compiler>
					<Add option="-m64" />
					<Add option="-Og" />
					<Add option="-g" />
					<Add option="-fno-omit-frame-pointer" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="-m64" />
				</Linker>
				<ExtraCommands>
					<Add after="objcopy --only-keep-debug ../../Debug/misc/T-Clock64.dll ../../Debug/misc/T-Clock64.dbg" />
					<Add after="objcopy --strip-debug --strip-unneeded --add-gnu-debuglink=../../Debug/misc/T-Clock64.dbg ../../Debug/misc/T-Clock64.dll" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="debug" targets="debug-64;debug-32;" />
			<Add alias="release" targets="release-64;release-32;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-D_UNICODE -DUNICODE" />
			<Add option="-fno-ident" />
			<Add option="-Wall" />
			<Add option="-Werror=declaration-after-statement" />
			<Add option="-fvisibility=hidden" />
			<Add option="-ffunction-sections" />
			<Add option="-fmerge-all-constants" />
			<Add option="-fno-asynchronous-unwind-tables" />
			<Add option="-fno-exceptions" />
			<Add option="-D_POSIX=1" />
			<Add option="-D_POSIX_C_SOURCE=200112L" />
			<Add option="-D__STDC_FORMAT_MACROS" />
			<Add option="-D__USE_MINGW_ANSI_STDIO=0" />
			<Add option="-D__MINGW_USE_VC2005_COMPAT" />
			<Add option="-DWINVER=0x0501" />
			<Add option="-DPSAPI_VERSION=1" />
			<Add option="-DWIN2K_COMPAT" />
		</Compiler>
		<Linker>
			<Add option="-municode" />
			<Add option="-static" />
			<Add option="-Wl,--gc-sections" />
			<Add library="advapi32" />
			<Add library="shell32" />
			<Add library="user32" />
			<Add library="ole32" />
			<Add library="gdi32" />
			<Add library="psapi" />
			<Add library="uuid" />
		</Linker>
		<ExtraCommands>
			<Add before="../common/AutoVersion --git . ../common/version.h" />
		</ExtraCommands>
